<?xml version="1.0" encoding="UTF-8"?>
<server description="new server">

    <!-- Enable features -->
    <featureManager>
    <feature>jsp-2.2</feature>
    <feature>cdi-1.0</feature>
    <feature>concurrent-1.0</feature>
